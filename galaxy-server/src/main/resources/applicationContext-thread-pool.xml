    <!-- 线程池 -->
    <bean id="uploadFileExecutor" class="org.springframework.scheduling.concurrent.ThreadPoolTaskExecutor">
        <!-- 线程池维护的核心线程数量 -->
        <property name="corePoolSize" value="20"/>
        <!-- 线程池维护线程的最大数量 -->
        <property name="maxPoolSize" value="50"/>
        <!-- 线程池所使用的缓冲队列 -->
        <property name="queueCapacity" value="30"/>
        <!-- 线程池维护线程所允许的空闲时间 -->
        <property name="keepAliveSeconds" value="30000"/>
    </bean>

</beans>